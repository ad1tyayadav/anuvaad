{
  "$schema": "https://lingo.dev/schema/i18n.json",
  "version": "1.10",
  "locale": {
    "source": "en",
    "targets": [
      "ru"
    ]
  },
  "buckets": {
    "json": {
      "include": [
        "app/locales/[locale].json"
      ]
    },
    "markdown": {
      "include": [
        "app/docs/[locale].md"
      ]
    }
  }
}